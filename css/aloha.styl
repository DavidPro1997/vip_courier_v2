.prebusqueda
    position absolute  
    margin 0
    padding 0.5em 0.7em
    top 0 
    left 0 
    display: none 
    z-index: 5000
    list-style none
    background white
    border 1px solid rgba(0,0,0,0.1)
    border-radius: 0.5em

    li  
        margin 0
        padding 0
        display block 
        border-bottom 1px solid rgba(0,0,0,0.05)

        a  
            margin 0
            padding 0.1em 0
            text-decoration none  
            font-size: 11px
            color #444  

            span
                margin 0
                padding 0
    
    li:last-child
        border-bottom: 0
    
    li:hover 

        a  
            color #9AC31C